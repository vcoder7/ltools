{
    "name": "vcoder7/ltools",
    "description": "Laravel helper tools",
    "keywords": [
        "php", "laravel", "artisan", "command", "commands"
    ],
    "type": "laravel-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vcoder7\\Ltools\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mladen Vukovic",
            "email": "mastertools32@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2 | ^8.3 | ^8.4",
        "illuminate/console": "^7.0 | ^8.0 | ^9.0 | ^10.0 | ^11.0 | ^12.0",
        "illuminate/support": "^12.19"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vcoder7\\Ltools\\PackageServiceProvider"
            ]
        }
    }
}
