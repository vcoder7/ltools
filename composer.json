{
    "name": "vcoder7/ltools",
    "description": "Laravel helper tools",
    "keywords": [
        "php", "laravel", "artisan", "command", "commands"
    ],
    "type": "laravel-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vcoder7\\Ltools\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mladen Vukovic",
            "email": "mastertools32@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vcoder7\\Ltools\\PackageServiceProvider",
                "Vcoder7\\Ltools\\MacroServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.1.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0|^12.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0|^10.0",
        "laravel/pint": "^1.27"
    },
    "scripts": {
        "pint": "pint",
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
